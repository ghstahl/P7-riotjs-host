{
  "name": "webpack-bootstrap3-jquery",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "fonts": "cpx node_modules/bootstrap/fonts/**/*   dist/fonts  -v",
    "json": "cpx ./app/*.json                         dist       -v",
    "html": "cpx ./app/*.html                         dist       -v",
    "public": "cpx ./dist/**/*                        public    -v",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:prod": "webpack --env=prod --progress --profile --colors",
    "build:library": "webpack --env=library --progress --profile --colors",
    "postBuild": "npm-run-all html json fonts",
    "serve": "webpack-dev-server --env=dev --hot",
    "dev:0": "npm-run-all build:dev postBuild",
    "dev": "npm-run-all dev:0 serve",
    "prod": "npm-run-all library dev:0 build:prod postBuild public",
    "library": "npm-run-all build:library  "
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs",
    "riot"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-riot": "^1.1.0",
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-config-rallycoding": "^3.2.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.1",
    "jquery": "^3.2.1",
    "loader-utils": "^1.1.0",
    "npm-run-all": "^4.0.2",
    "nprogress": "^0.2.0",
    "riot": "^3.5.1",
    "riot-route": "^3.1.1",
    "riot-tag-loader": "^1.0.0",
    "riotcontrol": "0.0.3",
    "riotjs-loader": "^4.0.0",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "whatwg-fetch": "^2.0.3"
  }
}
